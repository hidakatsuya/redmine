<%= turbo_frame_tag dom_id(starable, :stars) do %>
  <% if star = starable.stars.find_by(user: User.current) %>
    <%= link_to star_path(star, starable_id: starable), data: { turbo_method: :delete } do %>
      Star<%= starable.stars.size %>
    <% end %>
  <% else %>
    <%= link_to stars_path(starable_id: starable), data: { turbo_method: :post } do %>
      Star<%= starable.stars.size %>
    <% end %>
  <% end %>
<% end %>
